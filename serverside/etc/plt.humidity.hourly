cat<<EOF | gnuplot
set xdata time
set timefmt "%Y-%m-%dT%H:%M:%S"
set format x "%H:%M"
set timefmt "%Y-%m-%dT%H:%M:%S"
set datafile separator ","
set grid
set ylabel "humidity (%)"
set xlabel "Time of day (24h)"
set terminal png size 1200,300 enhanced font "Helvetica,8"
set output 'humidity.png'
plot "measurements" using 1:3 with lines title "Humidity `date +%Y-%m-%d`"
EOF
